package com.ljs.prob;

public class ReverseInteger {
	public int reverse(int x) {
        String s = String.valueOf(x);
		String ans = "";
		int result = 1;
		for(int i= s.length()-1; i>=0;i--) {
            if(s.charAt(i)>='0'){
             ans += s.charAt(i)+"";    
            } else {
            	result=-1;
            }
		}
	
		try {
			result = Integer.parseInt(ans);
		} catch(Exception e) {
			return 0;
		}
		return result;
	}
}
