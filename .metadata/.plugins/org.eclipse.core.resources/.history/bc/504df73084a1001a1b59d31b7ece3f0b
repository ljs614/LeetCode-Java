package com.ljs.etc;

public class E1 {
// 재귀라는데 ....
//	input 3
//	output 123
//	       132
//	       213
//	       231
//	       312
//	       321
	
	public void solution(int n) {
		int[] arr = new int[n];
		int i=0;
		while(i<n) {
			arr[i]=++i;
		}
	}
	
	public String permutation(int n, int[] arr) {
		
		return "";
	}
}
