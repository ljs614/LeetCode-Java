package com.ljs.etc;

import java.util.ArrayList;
import java.util.List;

public class E1 {
// 재귀라는데 ....
//	input 3
//	output 123
//	       132
//	       213
//	       231
//	       312
//	       321
	
	
	public void solution(int n) {
		
		int[] arr = new int[n];
		
		for(int i=0; i<n; i++) {
			arr[i]=i+1;
		}
		
		for(int i=0; i<n; i++) {
			List<String> list = permutation(i+1, arr);
			for(String s : list) {
				System.out.println(arr[i]+s);
			}
		}
		
	}
	
	public List<String> permutation(int n, int[] arr){
		List<String> list = new ArrayList<>();
		
		return list;
	}
	
}
